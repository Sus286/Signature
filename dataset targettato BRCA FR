io\n","TRIPLETS = json.loads(contenuto_file)"]},{"cell_type":"code","source":["len(TRIPLETS)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"TTkj-LafVXdp","executionInfo":{"status":"ok","timestamp":1706683915593,"user_tz":-60,"elapsed":587,"user":{"displayName":"Susanna Pagani","userId":"12094197595800410608"}},"outputId":"e6928b91-1175-485b-d927-e089bc743c3e"},"execution_count":6,"outputs":[{"output_type":"execute_result","data":{"text/plain":["1000"]},"metadata":{},"execution_count":6}]},{"cell_type":"code","source":["import pandas as pd\n","\n","TRIPLETS_DF = pd.DataFrame.from_dict(TRIPLETS, orient='index')\n"],"metadata":{"id":"KhWCvLNMV_pV","executionInfo":{"status":"ok","timestamp":1706683920863,"user_tz":-60,"elapsed":385,"user":{"displayName":"Susanna Pagani","userId":"12094197595800410608"}}},"execution_count":7,"outputs":[]},{"cell_type":"code","source":["TRIPLETS_DF"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":424},"id":"xj2Kg2RaWOrr","executionInfo":{"status":"ok","timestamp":1706683924400,"user_tz":-60,"elapsed":631,"user":{"displayName":"Susanna Pagani","userId":"12094197595800410608"}},"outputId":"e4cf58d4-3131-4041-d67c-724ea3660dfd"},"execution_count":8,"outputs":[{"output_type":"execute_result","data":{"text/plain":["       AAA    AAC    AAG    AAT    ACA    ACC    ACG    ACT    AGA    AGC  \\\n","0    80178  38136  56837  50576  54416  39357  13738  43385  60937  53055   \n","1    39705  18549  27346  25142  25893  18564   6479  21022  29655  24674   \n","2    40763  19517  29107  26205  27869  20068   7006  22331  31710  26984   \n","3    81316  38550  57671  51436  54365  40296  14093  44054  61310  53279   \n","4    55729  26176  39372  35793  36401  26387   9426  29630  41630  35818   \n","..     ...    ...    ...    ...    ...    ...    ...    ...    ...    ...   \n","995  77863  36597  54263  49499  51260  37147  13319  41695  58586  50587   \n","996  55244  26178  38824  34667  36129  26468   9448 