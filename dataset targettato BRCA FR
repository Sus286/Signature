ge(10000):\n","    optimizer.zero_grad()\n","    outputs = net(input_data)\n","    loss = criterion(outputs, output_data)\n","    loss.backward()\n","    optimizer.step()\n","\n","    if epoch % 10 == 0:\n","        print(f'Epoch {epoch+1}/{10000}, Loss: {loss.item()}')\n","\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"aEnRQoi2e_Te","executionInfo":{"status":"ok","timestamp":1706713349268,"user_tz":-60,"elapsed":6525107,"user":{"displayName":"Susanna Pagani","userId":"12094197595800410608"}},"outputId":"e4fda4f1-c2c0-49d5-e542-60f1e6f321e0"},"execution_count":100,"outputs":[{"output_type":"stream","name":"stdout","text":["Epoch 1/10000, Loss: 1.6680646971520914\n","Epoch 11/10000, Loss: 1.6061623536245333\n","Epoch 21/10000, Loss: 1.5231950718296543\n","Epoch 31/10000, Loss: 1.403275074423555\n","Epoch 41/10000, Loss: 1.2456467399378057\n","Epoch 51/10000, Loss: 1.0749455109732686\n","Epoch 61/10000, Loss: 0.9252257315787603\n","Epoch 71/10000, Loss: 0.822533144930664\n","Epoch 81/10000, Loss: 0.7723816356282528\n","Epoch 91/10000, Loss: 0.7534620495778022\n","Epoch 101/10000, Loss: 0.7428109797594539\n","Epoch 111/10000, Loss: 0.7326592882381777\n","Epoch 121/10000, Loss: 0.7226007050008196\n","Epoch 131/10000, Loss: 0.7121719024863534\n","Epoch 141/10000, Loss: 0.7013281303753272\n","Epoch 151/10000, Loss: 0.6905988841000381\n","Epoch 161/10000, Loss: 0.6805721177167563\n","Epoch 171/10000, Loss: 0.6716894818202411\n","Epoch 181/10000, Loss: 0.6641596550037121\n","Epoch 191/10000, Loss: 0.6580195171800155\n","Epoch 201/10000, Loss: 0.6531320698686641\n","Epoch 211/10000, Loss: 0.6492678838568519\n","Epoch 221/10000, Loss: 0.6461687639786902\n","Epoch 231/10000, Loss: 0.6436069047866374\n","Epoch 241/10000, Loss: 0.6414034454962122\n","Epoch 251/10000, Loss: 0.6394350126230753\n","Epoch 261/10000, Loss: 0.6376175404406187\n","Epoch 271/10000, Loss: 0.6358955333149968\n","Epoch 281/10000, Loss: 0.6342335841161605\n","Epoch 291/10000, Loss: 0.